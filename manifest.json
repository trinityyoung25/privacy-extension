{
  "manifest_version": 3,
  "name": "I330 Ad and Tracker Blocker",
  "version": "1.0",
  "description": "Blocks known trackers and logs data collection attempts.",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "activeTab"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
  "default_popup": "popup.html"
}
  }

